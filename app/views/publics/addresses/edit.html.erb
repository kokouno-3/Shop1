<h2>配送先編集</h2>
  <%= form_with model: @address, url: address_path(@address.id) ,method: :patch , local: true do |f| %>
    <p>郵便番号(ハイフンなし)</p>
      <%= f.text_field :postcode %>

    <p>住所</p>
      <%= f.text_field :address %>

    <p>宛名</p>
      <%= f.text_field :name %>
      <%= f.submit '編集する' %>
  <% end %>
